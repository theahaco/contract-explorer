{
	"extends": ["@aha-co/config/typescript.lib"],
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules", "dist", "**/*.test.ts", "**/*.test.tsx"],
	"compilerOptions": {
